      <script>
        function checkForm(){
            var set = document.getElementsByClassName("color")
if(set[0].value!=="" && set[1].value.length==10 && set[2].value!==""){
senRequ();
var formContainer = document.getElementsByClassName("form-container")
formContainer[0].innerHTML="<h2>THANK YOU</h2>"

}
else{
alert("Please fill the form with proper name, 10 digit no of contact no. Course name")
}
        }
async function senRequ(){

var set = document.getElementsByClassName("color")

var string = { "Name": set[0].value,
"Contact": set[1].value,
"Course": set[2].value,
}


 fetch('https://sheetdb.io/api/v1/qqq35gbtvsqa3', {
method: "POST",
headers: {
                'Content-Type': 'application/json'
            },
body: JSON.stringify(string),
})
.then((res) => res.json())

.then((json) => alert("Congratulation for joining with us"+JSON.stringify(json)));



}
      </script>
                  
